<div class="container"><br>
    <div class="jumbotron">
        <h1>Plan Your Wedding</h1>
         @model WeddingPlanner.Models.AddWedding
        <form asp-action="AddWedding" method="post">
            <label asp-for="WedderOne"></label>
            <input asp-for="WedderOne" class="form-control">
            <span asp-validation-for="WedderOne" class="text-danger"></span><br>

            <label asp-for="WedderTwo"></label>
            <input asp-for="WedderTwo" class="form-control">
            <span asp-validation-for="WedderTwo" class="text-danger"></span><br>

            <label asp-for="Date"></label>
            <input type="date" asp-for="Date" class="form-control">
            <span asp-validation-for="Date" class="text-danger"></span><br>

            <label asp-for="Address"></label>
            <input asp-for="Address" class="form-control">
            <span asp-validation-for="Address" class="text-danger"></span><br>

            <input type="submit" value="Create" class="btn btn-success">
        </form>
    </div>
</div>